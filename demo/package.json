    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "esbuild": "^0.8.55"
  },
  "browserslist": [
    "last 2 years"
  ]
